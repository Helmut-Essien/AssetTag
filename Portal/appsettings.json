{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Api": {
    "BaseUrl": "https://localhost:7135/"
  },
  "Dashboard": {
    "EnableCaching": true,
    "CacheDurationMinutes": 5,
    "AutoRefreshIntervalMinutes": 2,
    "ChartAnimation": true,
    "LazyLoading": true
  },
  "Performance": {
    "MaxParallelRequests": 10,
    "RequestTimeoutSeconds": 30
  },
  "AllowedHosts": "*"
}
